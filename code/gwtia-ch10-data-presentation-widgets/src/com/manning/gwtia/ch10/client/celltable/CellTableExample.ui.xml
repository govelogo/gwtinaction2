<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ct="urn:import:com.google.gwt.user.cellview.client">


<ui:style>
.panel {
	width: 100%;
	height: 100%;
}
</ui:style>

<g:DockLayoutPanel unit="PX" addStyleNames="{style.panel}">
   <g:north size="22">
   	<g:Label>Cell Table Example</g:Label>
   </g:north>
   <g:south size="33">
   		<ct:SimplePager ui:field="pager" />
   </g:south>
   <g:center>
   		<g:ResizeLayoutPanel>
   		<g:CustomScrollPanel>
	   			<ct:CellTable ui:field="cellTable" />
	   			</g:CustomScrollPanel>
   		</g:ResizeLayoutPanel>
   </g:center>
</g:DockLayoutPanel>
</ui:UiBinder>