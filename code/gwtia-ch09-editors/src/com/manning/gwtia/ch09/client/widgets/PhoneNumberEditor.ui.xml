
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../common.css"/>
	<g:HTMLPanel>
		<g:FlowPanel addStyleNames="{style.flowpanel}" height="229px">
			<g:FlowPanel>
				<g:Label addStyleNames="{style.label}">Phone type:</g:Label>				
				<g:TextBox ui:field="type" textAlignment="ALIGN_LEFT" visibleLength="30" maxLength="30"></g:TextBox>
			</g:FlowPanel>
			<g:FlowPanel height="75px">	
				<g:Label addStyleNames="{style.label}">Phone number:</g:Label>
				<g:TextBox ui:field="number" textAlignment="ALIGN_LEFT" visibleLength="30" maxLength="30"></g:TextBox>
		</g:FlowPanel>	
	</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 